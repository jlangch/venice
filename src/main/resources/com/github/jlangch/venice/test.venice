;;;; Test Venice classpath script


(ns test)

(defmacro test/sum [x y] `(+  x ~y))

(defn test/add [x y] (+ x y))

(defn test/test-fn [x]
  (str "test: " x))


(defn- test/test-fn-private [x]
  (str "test: " x))


(defn- test/error [msg]
  msg)


(defn test/unexpected-error [msg]
  (test/error (str "Unexpected " msg)))


;; used by macroexpand unit test
(defn test/test-body-with-macro [] (when true 1))
(defmacro test/expand-time [] (str (time/local-date-time)))
(defmacro test/macro-ns-expand [] (str *ns*))
(defmacro test/macro-ns-runtime [] `(str *ns*))
