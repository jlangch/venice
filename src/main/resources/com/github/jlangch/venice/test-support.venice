;;;; Test support for Venice unit tests


(ns test-support)

(defmacro test-support/sum [x y] `(+  ~x ~y))

(defn test-support/add [x y] (+ x y))

(defn test-support/test-fn [x]
  (str "test: " x))


(defn- test-support/test-fn-private [x]
  (str "test: " x))


(defn- test-support/error [msg]
  msg)


(defn test-support/unexpected-error [msg]
  (test-support/error (str "Unexpected " msg)))


;; used by macroexpand unit test
(defn test-support/test-body-with-macro [] (when true 1))
(defmacro test-support/expand-time [] (str (time/local-date-time)))
(defmacro test-support/macro-ns-expand [] (str *ns*))
(defmacro test-support/macro-ns-runtime [] `(str *ns*))
